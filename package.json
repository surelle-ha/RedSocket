	"description": "asd",
	"main": "server.js",
	"scripts": {
		"engine:serve": "nodemon engine.js",
		"client:serve": "vite",
		"client:build": "vite build",
		"client:preview": "vite preview",
		"server:test": "jest --watch",
		"server:serve": "cross-env SERVER_ENV=production nodemon server.js",
		"pm2": "pm2 monit",
		"pm2:dev": "pm2 start pm2.config.js",
		"pm2:prod": "pm2 start pm2.config.js --env production",
		"pm2:restart": "pm2 restart pm2.config.js",
		"pm2:stop": "pm2 stop pm2.config.js",
		"pm2:delete": "pm2 delete pm2.config.js",
		"db:up": "npm run db:create && npm run db:migrate",
		"db:reset": "npm run db:drop && npm run db:up && npx sequelize db:seed:all",
		"db:create": "npx sequelize db:create",
		"db:drop": "npx sequelize db:drop",
		"db:migrate": "npx sequelize db:migrate",
		"db:rollback": "npx sequelize db:migrate:undo",
		"db:rollback:all": "npx sequelize db:migrate:undo:all",
		"db:migrate:create": "npx sequelize-cli migration:generate --name"
	},
	"jest": {
		"testEnvironment": "node"
	},
